<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#62A57E">

    <Button
        android:id="@+id/buttonGoExam"
        android:layout_width="273dp"
        android:layout_height="56dp"
        android:layout_marginTop="88dp"
        android:layout_marginBottom="344dp"
        android:backgroundTint="#FFAB00"
        android:text="Create Exam"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonChooseSettings"
        app:strokeColor="#FFFFFF" />

    <Button
        android:id="@+id/buttonChooseSettings"
        android:layout_width="284dp"
        android:layout_height="59dp"
        android:layout_marginTop="184dp"
        android:backgroundTint="#43A047"
        android:text="Chose Exam Setting"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>